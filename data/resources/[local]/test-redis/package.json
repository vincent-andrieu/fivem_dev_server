        "@typescript-eslint/parser": "^8.7.0",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "prettier": "^3.3.3",
        "rollup": "^4.22.5",
        "rollup-plugin-typescript2": "^0.36.0",
        "typescript": "^5.6.2"
    },
    "dependencies": {
        "@citizenfx/client": "^2.0.10048-1",
        "@citizenfx/server": "^2.0.10048-1",
